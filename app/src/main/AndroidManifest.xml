<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dariapro.trainyourcountingskills">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents.Light.Bridge">

        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.dariapro.trainyourcountingskills.activity.ScoresActivity"
            android:label="@string/app_name"
            android:parentActivityName=".activity.ScoresActivity">
        </activity>
        <activity android:name="com.dariapro.trainyourcountingskills.activity.LevelListActivity"
            android:label="@string/app_name"
            android:parentActivityName=".activity.LevelListActivity">
        </activity>
        <activity android:name="com.dariapro.trainyourcountingskills.activity.QuestionListActivity"
            android:label="@string/app_name"
            android:parentActivityName=".activity.QuestionListActivity">
        </activity>
        <activity android:name="com.dariapro.trainyourcountingskills.activity.SimpleQuestionPagerActivity"
            android:label="@string/app_name"
            android:parentActivityName=".activity.SimpleQuestionPagerActivity">
        </activity>
        <activity android:name="com.dariapro.trainyourcountingskills.activity.CategoryActivity"
            android:label="@string/app_name"
            android:parentActivityName=".activity.CategoryActivity">
        </activity>
        <activity android:name="com.dariapro.trainyourcountingskills.activity.RandomQuestionStartActivity"
            android:label="@string/app_name"
            android:parentActivityName=".activity.RandomQuestionPagerActivity">
        </activity>
        <activity android:name="com.dariapro.trainyourcountingskills.activity.RandomQuestionPagerActivity"
            android:label="@string/app_name"
            android:parentActivityName=".activity.RandomQuestionPagerActivity">
        </activity>
        <activity android:name="com.dariapro.trainyourcountingskills.activity.AnswerActivity"
            android:label="@string/app_name"
            android:parentActivityName=".activity.AnswerActivity">
        </activity>

    </application>

</manifest>